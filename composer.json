{
    "name": "vinogradova/sanitizer",
    "type": "library",
    "description": "Sanitizer - валидатор для струкртур данных из формата [key => value]",
    "autoload": {
        "psr-4": {
            "Sanitizer\\": "src/",
            "Sanitizer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vinogradova Tatyana",
            "email": "tanyavinogradova1@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.26"
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
